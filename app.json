{
  "expo": {
    "jsEngine": "hermes",
    "name": "react-native-codebase",
    "slug": "react-native-codebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "reactnativecodebase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dennisthandy.reactnativecodebase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dennisthandy.reactnativecodebase",
      "enableDangerousExperimentalLeanBuilds": true,
      "enableProguardInReleaseBuilds": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Poppins-Regular.otf",
            "./src/assets/fonts/Poppins-Medium.otf",
            "./src/assets/fonts/Poppins-Bold.otf",
            "./src/assets/fonts/Poppins-SemiBold.otf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourceslnReleaseBuilds": true,
            "useLegacyPackaging": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9c3d2377-462c-4e0f-acc2-29f46ef69caf"
      }
    }
  }
}
